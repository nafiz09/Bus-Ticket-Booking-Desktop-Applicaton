<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.AdminUI">
    <children>
        <AnchorPane prefHeight="500.0" prefWidth="900.0">
            <children>
                <BorderPane fx:id="borderPane" prefHeight="500.0" prefWidth="900.0">
                    <left>
                        <VBox prefHeight="458.0" prefWidth="130.0" style="-fx-background-color: #cbfff0;" BorderPane.alignment="CENTER">
                            <children>
                                <Label fx:id="loggedIn" prefHeight="37.0" prefWidth="130.0">
                                    <font>
                                        <Font name="System Bold" size="14.0" />
                                    </font>
                                    <graphic>
                                        <ImageView fitHeight="30.0" fitWidth="35.0" pickOnBounds="true" preserveRatio="true">
                                            <image>
                                                <Image url="@admin_1246350.png" />
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </Label>
                                <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="131.0" text="Dashboard" />
                                <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="131.0" text="Booking Details" />
                                <Button fx:id="addBus" mnemonicParsing="false" onAction="#onActionAddBus" prefHeight="25.0" prefWidth="131.0" text="Add Bus" />
                                <Button fx:id="logOut" mnemonicParsing="false" onAction="#onActionlogout" prefHeight="26.0" prefWidth="130.0" text="Log Out">
                                    <cursor>
                                        <Cursor fx:constant="HAND" />
                                    </cursor>
                                    <graphic>
                                        <ImageView fitHeight="20.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                                            <image>
                                                <Image url="@disconnect_logout_exit-512.png" />
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </Button>
                            </children>
                            <effect>
                                <InnerShadow />
                            </effect>
                        </VBox>
                    </left>
                    <top>
                        <HBox prefHeight="34.0" prefWidth="900.0" style="-fx-background-color: #01feb4;" BorderPane.alignment="CENTER" />
                    </top>
                    <center>
                        <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                            <children>
                                <TableView prefHeight="466.0" prefWidth="771.0">
                                    <columns>
                                        <TableColumn prefWidth="137.99998474121094" text="Bus Id" />
                                        <TableColumn prefWidth="159.33331298828125" text="Name of Bus" />
                                        <TableColumn prefWidth="159.33331298828125" text="Type of Bus" />
                                        <TableColumn prefWidth="159.33331298828125" text="Seat Capacity" />
                                        <TableColumn prefWidth="159.33331298828125" text="Status" />
                                    </columns>
                                </TableView>
                            </children>
                        </AnchorPane>
                    </center>
                </BorderPane>
            </children>
        </AnchorPane>
    </children>
